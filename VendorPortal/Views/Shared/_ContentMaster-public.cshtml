        function GetWindowHeight() {
            if (window.innerHeight) {
                return window.innerHeight;
            }
            return document.documentElement.clientHeight;
        }
        function GetPopupWindowHeight() {
            var winHeight;
            if (window.innerHeight) {
                winHeight = window.innerHeight;
            }
            winHeight = document.documentElement.clientHeight;

            return winHeight - 100;
        }

        var isdirty = false;
        function ClearDirtyFlag(container) {
            var popupIndex = $.inArray(container, dirtyForms);
            if (popupIndex >= 0) {
                dirtyForms.splice(popupIndex, 1);
            }
        }
        function IsPageDirty() {

            var popupIndex = $.inArray("popupcontainer", dirtyForms);
            if (popupIndex >= 0) {
                var answer = confirm('There are some unsaved changes on the form. Click Cancel to stay on the page or click OK to discard your changes and navigate away.');
                if (answer) {
                    dirtyForms.splice(popupIndex, 1);
                    return true;
                }
                else {
                    return false;
                }
            }
            else {
                return true;
            }
        }
        // Set the following variable to false if dirty page checks are not required
        var handleDirtyPage = true;
        var dirtyForms = [];
        function MarkPageAsDirty(ele) {
            // Get the closest form and see if we already have marked it as dirty.
            var form = $(ele).closest('.pagecontainer');
            var formId = "pagecontainer";
            if (form.length > 0) {
                if ($.inArray(formId, dirtyForms) == -1) {
                    dirtyForms.push(formId);
                }
            }
            else {
                form = $(ele).closest('.popupcontainer');
                formId = "popupcontainer";
                if (form.length > 0 && $.inArray(formId, dirtyForms) == -1) {
                    dirtyForms.push(formId);
                }
            }
        }
        var phoneNumberCalled = '';
        $(function () {
            $("input,textarea,select").live("change", function (e) {

                //KB: Stickynote should be excluded from dirty page checks
                if ($(e.currentTarget).attr("class") != "stickyNote-editable" && handleDirtyPage) {
                    MarkPageAsDirty(this);

                }

            });

        });

        window.onbeforeunload = confirmExit;
        function confirmExit() {

            var len = dirtyForms.length;
            if (len > 0)
                return "You have attempted to leave this page.  If you have made any changes to the fields without clicking the Save button, your changes will be lost.  Are you sure you want to exit this page?";
        }

