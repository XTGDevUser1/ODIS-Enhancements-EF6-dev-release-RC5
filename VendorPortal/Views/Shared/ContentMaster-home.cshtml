</body>

</html>

<script type="text/javascript">
    $(function () {


        $.reject({
            reject: {
                all: false,
                msie5: true, msie6: true, msie7: true, msie8: true,
                firefox1: true,
                konqueror1: true,
                chrome1: true,
                safari2: true,
                opera7: true
                /*
                * Possibilities are endless...
                *
                * // MSIE Flags (Global, 5-8)
                * msie, msie5, msie6, msie7, msie8,
                * // Firefox Flags (Global, 1-3)
                * firefox, firefox1, firefox2, firefox3,
                * // Konqueror Flags (Global, 1-3)
                * konqueror, konqueror1, konqueror2, konqueror3,
                * // Chrome Flags (Global, 1-4)
                * chrome, chrome1, chrome2, chrome3, chrome4,
                * // Safari Flags (Global, 1-4)
                * safari, safari2, safari3, safari4,
                * // Opera Flags (Global, 7-10)
                * opera, opera7, opera8, opera9, opera10,
                * // Rendering Engines (Gecko, Webkit, Trident, KHTML, Presto)
                * gecko, webkit, trident, khtml, presto,
                * // Operating Systems (Win, Mac, Linux, Solaris, iPhone)
                * win, mac, linux, solaris, iphone,
                * unknown // Unknown covers everything else
                */
            },
            display: ['firefox', 'chrome', 'msie'], // Displays only firefox, chrome, and opera
            closeCookie: false, // Set cookie to remmember close for this session
            header: 'Your browser needs to be updated.', // Header Text
            paragraph1: 'You must update to use the new Pinnacle Partner Portal', // Paragraph 1
            paragraph2: 'Please install one of these modern browsers.',
            closeMessage: 'Please update your web browser and then return to this page.' // Message below close window link
        }); // Customized Browsers
    });
</script>


