<body class="">
    <header role="banner" id="title-bar-Public">
        <h1 class="no-txt" id="logo"><a href="~/">Pinnacle Partner Network</a></h1>
        <div class="flexnav-menu-button" id="flexnav-menu-button">Menu<span class="touch-button active"></span></div>
        <nav id="flexnav" class="flexnav2">
            <ul>
                <li><a href="/">Home</a></li>
                <li><a href="@Url.Action("Index", "Apply", new { area = "" })">Apply to become a Partner</a></li>
                <li><a href="@Url.Action("Index", "ContactUs", new { area = "" })">Contact Us</a></li>
                <li><a href="@Url.Action("Index", "Register", new { area = "" })" class="btn btn-large tnav">Register</a></li>
                <li><a href="@Url.Action("Login", "Account", new { area = "" })" class="btn btn-primary btn-large tnav">Log In</a></li>
            </ul>
        </nav>

    </header>

    <div id="section-top">

    </div>
    <div class="container-global">

        @RenderBody()



        <footer class="main center">
            <div class="container">

                <div class="gap gap-small"></div>


                <div class="gap gap-small"></div>	<small> &copy; @Html.Raw(DateTime.Now.ToString("yyyy")) Pinnacle Motor Club. All rights reserved. <a href="@Url.Action("Index", "PrivacyPolicy", new { area = "" })">Privacy Policy</a></small>
