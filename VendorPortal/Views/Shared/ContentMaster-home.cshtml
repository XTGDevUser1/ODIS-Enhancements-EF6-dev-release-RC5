        <script src="@Url.Content("~/Content/home/js/flexnav.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/countdown.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/magnific.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/mediaelement.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/fitvids.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/gridrotator.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/fredsel.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/backgroundsize.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/superslides.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/one-page-nav.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/scroll-to.js")"></script>
        <script src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>
        <script src="@Url.Content("~/Content/home/js/gmap3.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/tweet.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/mixitup.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/mail.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/transit-modified.js")"></script>
        <script src="@Url.Content("~/Content/home/js/layerslider-transitions.min.js")"></script>
        <script src="@Url.Content("~/Content/home/js/layerslider.js")"></script>
        <script src="@Url.Content("~/Content/home/js/custom.js")"></script>
        <link rel="stylesheet" href="@Url.Content("~/Content/jquery.reject.css")" />
        <script src="@Url.Content("~/Scripts/jquery.browser.min.js")" type="text/javascript"></script>

    </div>

    <!-- Help Modal -->
    <div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="myModalLabel">
                Registration and Log In Help
            </h3>
        </div>
        <div class="modal-body">
            @Html.Partial("_RegistrationHelpContent")
        </div>
        <div class="modal-footer">
            <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
        </div>
    </div><!-- end help modal -->
    <!--Google Analytics Code-->
    <script>
        (function (i, s, o, g, r, a, m) {
