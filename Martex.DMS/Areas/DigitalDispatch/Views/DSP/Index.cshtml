<script type="text/javascript">
    $("#btnSubmitDSP").click(function () {
        $.ajax({
            type: 'POST',
            url: '@Url.Action("SubmitDSP", "DSP", new { area = "DigitalDispatch" })',
            data: $("#formDSP").serializeArray(),
            traditional: true,
            cache: false,
            async: true,
            success: function (msg) {
                $("#DSPResponseMessage").html(msg.Data.Response);
                $("#DSPRequestMessage").html(msg.Data.Request);
                $("#divDSPResponse").show();
            }
        });
    });
    $(function () {
        $("#divDSPResponse").hide();
    });
</script>