@model Martex.DMS.BLL.Model.TempCCPModels.ImportCCFileResult
<div class="with-padding">
    <p>
        Import CC File Results
    </p>
    <table class="table simple-table">
        <tr>
            <td>
                Total records read
            </td>
            <td>
                @Model.TotalRecordRead
            </td>
        </tr>
        <tr>
            <td>
                Total records ignored
            </td>
            <td>
                @Model.TotalRecordIgnored
            </td>
        </tr>
        <tr>
            <td>
                Total credit cards added
            </td>
            <td>
                @Model.TotalCreditCardAdded
            </td>
        </tr>
         <tr>
            <td>
                Total detail transactions added
            </td>
            <td>
                @Model.TotalDetailTransactionAdded
            </td>
        </tr>
        <tr>
            <td>
                Total error records
            </td>
            <td>
                @Model.TotalErrorRecords
            </td>
        </tr>
    </table>
</div>
