<!-- Block UI -->
<script language="javascript" type="text/javascript">

    $(function () {
        $('html')
        .ajaxStart(function () {

            $.blockUI({ message: '<div class="ajax-load"/>'
                ,
                css: { background: 'transparent', border: '0px' }
            });
        })
        .ajaxStop(function () {

            $.unblockUI();
            $('html').css('cursor', 'pointer');

        })
        .ajaxError(function (err, errStatus) {
            var errorMessage = 'An error occurred while processing the request.';
            if (errStatus.status == 403) {
                Handle403(errStatus);
                return false;
            }
            try {
                var json = $.parseJSON(errStatus.responseText);
                if (json.Data == null) {
                    errorMessage = json.ErrorMessage;
                }
                else {
                    errorMessage = json.ErrorMessage + "<br/> Please contact administrator with the key [ " + json.Data + " ] for more details";
                }
            }
            catch (exception) {
                // possible error while parsing response as JSON
            }
            $.unblockUI();
            $('html').css('cursor', 'pointer');

            if (typeof (formWrapper) != "undefined") {
                formWrapper.clearMessages();
            }
            openAlertMessage(errorMessage);

        });
    });

    function Handle403(errStatus) {

        openAlertMessage("Session timed out");
        var json = JSON.parse(errStatus.responseText);
        window.location = json.Data;

    }
</script>
<script src="@Url.Content("~/Scripts/libs/tinycon.min.js")"></script>

<script language="javascript" type="text/javascript">
    function GetWindowHeight() {
        if (window.innerHeight) {
            return window.innerHeight;
        }
        return document.documentElement.clientHeight;
    }
    function GetPopupWindowHeight() {
        var winHeight;
        if (window.innerHeight) {
            winHeight = window.innerHeight;
        }
        winHeight = document.documentElement.clientHeight;

        return winHeight - 50;
    }

    var isdirty = false;
    function IsPageDirty() {
        if (isdirty) {

            var answer = confirm('There are some unsaved changes on the form. Click Cancel to stay on the page or click OK to discard your changes and navigate away.');
            if (answer) {
                isdirty = false;
                return true;
            }
            else {
                return false;
            }
        }
        else {
            return true;
        }
    }
    // Set the following variable to false if dirty page checks are not required
    var handleDirtyPage = true;
    $(function () {
        $("input,textarea,select").live("change",function (e) {
            //KB: Stickynote should be excluded from dirty page checks
            if ($(e.currentTarget).attr("class") != "stickyNote-editable" && handleDirtyPage) {                
                isdirty = true;
            }

        });
    });

    window.onbeforeunload = confirmExit;
    function confirmExit() {
        if (isdirty)
            return "You have attempted to leave this page.  If you have made any changes to the fields without clicking the Save button, your changes will be lost.  Are you sure you want to exit this page?";
    }

        
</script>
