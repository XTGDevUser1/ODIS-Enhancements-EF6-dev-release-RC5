@model Martex.DMS.DAL.Vehicle
<div class="with-padding">
    <div class="columns">
        <div class="twelve-columns">
            <form name="frm_ServiceAutoTab" id="frm_ServiceAutoTab" action="#">
            @Html.Partial("_BasicInformation", Model)
            @if ((bool)ViewBag.ShowCommercialVehicle)
            { 
              @Html.Partial("_AutoSpecifications", Model)
            }
            @Html.Partial("_WarrantyInformation", Model)
            </form>
        </div>
    </div>
</div>
<script type="text/javascript">    $(function () {
        $("#frm_ServiceAutoTab").validationEngine();
    });
</script>
