@model List<Martex.DMS.DAL.PreviousCallList>
@for (int i = 0; i < Model.Count; i++)
{
    <div class="callLogEntry" style="width: 100%; float: left; display: inline-table;
        margin-bottom: 10px; margin-top: 5px;" id="@("callLogEntry" + i.ToString())">
        <div class="callLogDetails" style="width: 50%; float: left;">
            <ul style="list-style-type: none; margin-left: 5px;">
                <li class="contactSource">@Model[i].ContactSourceName</li>
                <li class="company">@Model[i].Company</li>
                <li class="phonenumber">
                @Html.Editor("callLogEntry" + i.ToString(),"Phone", new {ReadOnly=true, Value=Model[i].PhoneNumber, CallType = "Vendor"})
                </li>
                <li class="talkedto">@Model[i].TalkedTo</li>
                <li class="contactAction">@Model[i].ContactActionName</li>
            </ul>
        </div>
        <div style="width: 48%; float: left;">
            <ul style="list-style-type: none; margin-left: 5px; margin-right: 5px;">
                <li>@Model[i].CreateDate.Value.ToShortDateString()</li>
                <li>@Model[i].CreateDate.Value.ToShortTimeString()</li>
                <li>@Model[i].CreateBy</li>
                <li><a class="CallbackFillDetails">Callback</a></li>
            </ul>
        </div>
    </div>
}
